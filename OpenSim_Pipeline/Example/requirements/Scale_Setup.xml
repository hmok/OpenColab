<?xml version="1.0" encoding="UTF-8" ?>
<OpenSimDocument Version="40000">
<ScaleTool name="subject01">
	

	<!--Mass of the subject in kg.  Subject-specific model generated by
	    scaling step will have this total mass.-->
	<mass>65</mass>
	<!--Height of the subject in mm.  For informational purposes only (not
	    used by scaling).-->
	<height>170</height>
	<!--Age of the subject in years.  For informational purposes only (not
	    used by scaling).-->
	<age>99</age>
	<!--Notes for the subject.-->
	<notes> This is an example setup file for scale.exe. </notes>
	
	<!--Specifies the name of the unscaled model (.osim) and the marker set.-->
	<GenericModelMaker name="">
		<!--Model file (.osim) for the unscaled model.-->
		<model_file> gait2392_simbodyHBM.osim</model_file>
		<!--Set of model markers used to scale the model. Scaling is done based on
		    distances between model markers compared to the same distances between
		    the corresponding experimental markers.-->
		<marker_set_file> markersHBM.xml </marker_set_file>
	</GenericModelMaker>
	
	<!--Specifies parameters for scaling the model.-->
	<ModelScaler name="">
		<!--Specifies the scaling method and order. Valid options are
		    'measurements', 'manualScale', singly or both in any sequence.-->
		<scaling_order> measurements manualScale </scaling_order>
		<!--Scale factors to be used for manual scaling.-->
		<ScaleSet file="subject01_Scale_ScaleSet.xml"/>
		<!--Specifies the measurements by which body segments are to be scaled.-->
		<MeasurementSet file="MU2392_Arms_Scale_MeasurementSet.xml"/>
		<!--TRC file (.trc) containing the marker positions used for
		    measurement-based scaling. This is usually a static trial, but doesn't
		    need to be.  The marker-pair distances are computed for each time step
		    in the TRC file and averaged across the time range.-->
		<marker_file> Calibration.trc </marker_file>
		<!--Time range over which to average marker-pair distances in the marker
		    file (.trc) for measurement-based scaling.-->
		<time_range> 1 2 </time_range>
		<!--Flag (true or false) indicating whether or not to preserve relative
		    mass between segments.-->
		<preserve_mass_distribution> true </preserve_mass_distribution>
		<!--Name of SIMM joint file to write when done scaling. If not specified,
		    a file will not be written.-->
		<output_joint_file> subject01_scaledOnly.jnt </output_joint_file>
		<!--Name of SIMM muscle file to write when done scaling. If not specified,
		    a file will not be written.-->
		<output_muscle_file> subject01_scaledOnly.msl </output_muscle_file>
		<!--Name of OpenSim model file (.osim) to write when done scaling.-->
		<output_model_file> subject01_scaledOnly.osim </output_model_file>
		<!--Name of file to write containing the scale factors that were applied
		    to the unscaled model (optional).-->
		<output_scale_file> subject01_scaleSet_applied.xml </output_scale_file>
	</ModelScaler>
	
	<!--Specifies parameters for placing markers on the model once a model is
	    scaled.-->
	<MarkerPlacer name="">
		<!--TRC file (.trc) containing the time history of experimental marker
		    positions (usually a static trial).-->
		<marker_file>Calibration.trc</marker_file>
		<!--Task set used to specify weights used in the IK computation of the
		    static pose.-->
		<IKTaskSet name="MU2392_Arms_Scale">
	<objects>
		<!-- Torso (5 markers) -->
		<IKMarkerTask name="T10"> <apply>true</apply> <weight>.0001</weight> </IKMarkerTask>
		<IKMarkerTask name="XIPH"> <apply>true</apply> <weight>.0001</weight> </IKMarkerTask>
		<IKMarkerTask name="JN"> <apply>true</apply> <weight>.0001</weight> </IKMarkerTask>
		<IKMarkerTask name="C7"> <apply>true</apply> <weight>1</weight> </IKMarkerTask>
		

		<!-- Pelvis (3 markers) -->
		<IKMarkerTask name="RASIS"> <apply>true</apply> <weight>10</weight> </IKMarkerTask>
		<IKMarkerTask name="LASIS"> <apply>true</apply> <weight>10</weight> </IKMarkerTask>
		 <IKMarkerTask name="RPSIS"> <apply>true</apply> <weight>10</weight> </IKMarkerTask>   
		 <IKMarkerTask name="LPSIS"> <apply>true</apply> <weight>10</weight> </IKMarkerTask>   
	

		<!-- Right femur (6 markers) -->
	 <IKMarkerTask name="RLTHI"> <apply>true</apply> <weight>.0001</weight> </IKMarkerTask>
		<IKMarkerTask name="RLEK"> <apply>true</apply> <weight>1</weight> </IKMarkerTask>
		<IKMarkerTask name="RMEK"> <apply>true</apply> <weight>1</weight> </IKMarkerTask>
		

		<!-- Right tibia (5 markers) -->
<IKMarkerTask name="RLSHA"> <apply>true</apply> <weight>.0001</weight> </IKMarkerTask>
		<IKMarkerTask name="RLM"> <apply>true</apply> <weight>10</weight> </IKMarkerTask>
		<IKMarkerTask name="RMM"> <apply>true</apply> <weight>10</weight> </IKMarkerTask> 
		

		<!-- Right foot (6 markers) -->
		<IKMarkerTask name="RHEE"> <apply>true</apply> <weight>10</weight> </IKMarkerTask>
		 <IKMarkerTask name="RMT5"> <apply>true</apply> <weight>5</weight> </IKMarkerTask>
		<IKMarkerTask name="RMT2"> <apply>true</apply> <weight>5</weight> </IKMarkerTask>
		<!-- <IKMarkerTask name="RP1MT"> <weight>5</weight> </IKMarkerTask>
		<IKMarkerTask name="RP5MT"> <weight>5</weight> </IKMarkerTask> 
		<IKMarkerTask name="RTOE"> <weight>10</weight> </IKMarkerTask>  -->

		<!-- Left femur  (6 markers) -->
		<IKMarkerTask name="LLTHI"> <apply>true</apply> <weight>.0001</weight> </IKMarkerTask>
		<IKMarkerTask name="LLEK"> <apply>true</apply> <weight>1</weight> </IKMarkerTask>
		<IKMarkerTask name="LMEK"> <apply>true</apply> <weight>1</weight> </IKMarkerTask>

		<!-- Left tibia (5 markers) -->
		<IKMarkerTask name="LLSHA"> <apply>true</apply> <weight>.0001</weight> </IKMarkerTask>
		<IKMarkerTask name="LLM"> <apply>true</apply> <weight>10</weight> </IKMarkerTask>
		<IKMarkerTask name="LMM"> <apply>true</apply> <weight>10</weight> </IKMarkerTask> 

		<!-- Left foot (6 markers) -->
		<IKMarkerTask name="LHEE"> <apply>true</apply> <weight>10</weight> </IKMarkerTask>
		 <IKMarkerTask name="LMT5"> <apply>true</apply> <weight>5</weight> </IKMarkerTask>
		<IKMarkerTask name="LMT2"> <apply>true</apply> <weight>5</weight> </IKMarkerTask>
		<!--<IKMarkerTask name="LP1MT"> <weight>1</weight> </IKMarkerTask>
		<IKMarkerTask name="LP5MT"> <weight>1</weight> </IKMarkerTask> 
		<IKMarkerTask name="LTOE"> <weight>10</weight> </IKMarkerTask> -->

				

		<IKCoordinateTask name="subtalar_angle_r">
      <apply> false </apply>
			<weight>    1000.00000000 </weight>
			<value_type> default_value </value_type>
		</IKCoordinateTask>
		<IKCoordinateTask name="mtp_angle_r">
      <apply> false </apply>
			<weight>    1000.00000000 </weight>
			<value_type> default_value </value_type>
		</IKCoordinateTask>
		<IKCoordinateTask name="subtalar_angle_l">
      <apply> false </apply>
			<weight>    1000.00000000 </weight>
			<value_type> default_value </value_type>
		</IKCoordinateTask>
		<IKCoordinateTask name="mtp_angle_l">
      <apply> false </apply>
			<weight>    1000.00000000 </weight>
			<value_type> default_value </value_type>
		</IKCoordinateTask>
		<!-- Helps keep static pose solution from having back bent too much -->
		<IKCoordinateTask name="lumbar_extension">
      <apply> false </apply>
			<weight>10.0</weight> 
			<value_type> manual_value </value_type>
			<value>0</value>
		</IKCoordinateTask>
    <IKCoordinateTask name="pelvis_tilt">
      <weight>1000.0</weight>
      <apply> false </apply>
      <value_type> manual_value </value_type>
      <value>0</value>
    </IKCoordinateTask>
	</objects>
</IKTaskSet>
		<coordinate_file> </coordinate_file>
		<!--Time range over which the marker positions are averaged.-->
		<time_range> 1 2 </time_range>
		<!--Name of the new SIMM Joint file (.jnt) after scaling and marker
		    placement (optional).-->
		<output_joint_file> subject01.jnt </output_joint_file>
		<!--Name of the SIMM muscle file (.msl) after scaling and marker placement
		    (optional).-->
		<output_muscle_file> subject01.msl </output_muscle_file>
		<!--Output OpenSim model file (.osim) after scaling and maker placement.-->
		<output_model_file> subject01_simbody.osim </output_model_file>
		<!--Name of the motion file (.mot) written after marker relocation
		    (optional).-->
		<output_motion_file> subject01_static_output.mot </output_motion_file>
	</MarkerPlacer>
	
</ScaleTool>

</OpenSimDocument>